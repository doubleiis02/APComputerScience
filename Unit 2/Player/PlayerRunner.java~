// driver for Player objects
// creating many objects from one class

public class PlayerRunner
{
  public static void main(String[] args)
  {
    Player p1 = new Player("Jiwon");
    Player p2 = new Player("Hamood");
    
    p2.setAttempts(5);
    p1.setAttempts(9);
    p2.setScore(4);
    p1.setScore(5);
    
    System.out.println(p1);
    System.out.println(p2);
    
    Player p3 = new Player("David");
    Player p4 = new Player("Alejandro");
    Player p5 = new Player("Leo");
    
    p3.setAttempts(3);
    p4.setAttempts(11);
    p5.setAttempts(7);
    p3.setScore(2);
    p4.setScore(4);
    p5.setScore(2);
    
    System.out.println(p3);
    System.out.println(p4);
    System.out.println(p5);
  }
}